{
  "name": "webdriverio-example",
  "version": "1.0.0",
  "description": "**Primary**: [Sauce Demo](https://www.saucedemo.com/) **Secondary**: [Automation Exercise](https://automationexercise.com/)",
  "homepage": "https://github.com/Luki1235512/WebdriverIO-Example#readme",
  "bugs": {
    "url": "https://github.com/Luki1235512/WebdriverIO-Example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luki1235512/WebdriverIO-Example.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "wdio": "wdio run ./wdio.conf.ts",
    "wdio:report": "npm run wdio && npm run allure:generate && npm run allure:open",
    "wdio:serve": "npm run wdio && npm run allure:serve",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "prepare": "husky",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results",
    "clean:old-history": "find allure-report/history -type f -mtime +10 -delete 2>/dev/null || true"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@wdio/allure-reporter": "^9.24.0",
    "@wdio/cli": "^9.24.0",
    "@wdio/cucumber-framework": "^9.24.0",
    "@wdio/globals": "^9.23.0",
    "@wdio/local-runner": "^9.24.0",
    "@wdio/spec-reporter": "^9.24.0",
    "@wdio/visual-service": "^9.1.4",
    "allure-commandline": "^2.36.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "expect-webdriverio": "^5.6.4",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.56.0"
  }
}
